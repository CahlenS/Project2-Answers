package AnswersPackage;

public class FibonacciNumbers {
	
	static int fib(int f) {
		
		if(f <= 1)
			return f;
		return fib(f-1) + fib(f-2);
		
	}
	
	void array(int arr[]) {
		
		int n = arr.length;
		for (int i = 0; i < n; ++i)
			System.out.print(arr[i] + " ");
		System.out.println();
		
	}
	
	

}
